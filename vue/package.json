      "vue-loader": "^15.7.0",
      "vue-template-compiler": "^2.6.10",
      "webpack": "^4.35.0",
      "px2rem-loader": "^0.1.9",
      "webpack-bundle-analyzer": "^3.3.2",
      "webpack-cli": "^3.3.0",
      "webpack-dev-server": "^3.4.1",
      "webpack-merge": "^4.2.1"
  }
