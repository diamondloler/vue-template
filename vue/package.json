{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "category" : "<%= category %>",
  "projectName" : "<%= projectName %>", 
  "testSVN" : "<%= testSVN %>",
  "prodSVN" : "<%= prodSVN %>",
  "description": "",
  "scripts": {
      "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.conf.js",
      "test": "cross-env NODE_ENV=development webpack --config ./build/webpack.test.conf.js",
      "prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.conf.js"
  },
  "sideEffects": [
      "*.css",
      "*.scss",
      "*.vue"
  ],
  "browserslist": [
      "Safari >= 6",
      "ie > 8"
  ],
  "dependencies": {
      "@babel/runtime-corejs2": "^7.4.5",
      
      "babel-plugin-transform-runtime": "^6.23.0",
      "babel-runtime": "^6.23.0",
      <% if(category == 'h5') { %>
      "mint-ui": "^2.2.13",
      <% }else{ %>
      "element-ui": "^2.10.1",
      <% } %>
      "vue": "^2.4.2",
      <% if(vuex_router) { %>
      "vue-router": "^2.3.1",
      "vuex": "^2.3.0",
      <% } %>
      "qs": "^6.6.0",
      "axios": "^0.19.0"
  },
  "devDependencies": {
      "@babel/core": "^7.4.5",
      "@babel/plugin-syntax-dynamic-import": "^7.2.0",
      "@babel/plugin-transform-runtime": "^7.4.4",
      "@babel/preset-env": "^7.4.5",
      "autoprefixer": "^9.5.1",
      "babel-eslint": "^10.0.1",
      "babel-loader": "^8.0.6",
      "babel-plugin-component": "^1.1.1",
      "clean-webpack-plugin": "^2.0.2",
      "cross-env": "^5.2.0",
      "css-loader": "^2.1.1",
      "eslint": "^5.16.0",
      "eslint-config-alloy": "^1.4.2",
      "eslint-plugin-vue": "^3.14.0",
      "file-loader": "^4.0.0",
      "html-webpack-plugin": "^3.2.0",
      "imagemin": "^6.1.0",
      "imagemin-pngquant": "^8.0.0",
      "img-loader": "^3.0.1",
      "jdists": "^2.2.4",
      "mini-css-extract-plugin": "^0.7.0",
      "node-sass": "^4.12.0",
      "optimize-css-assets-webpack-plugin": "^5.0.1",
      "postcss-loader": "^3.0.0",
      "process": "^0.11.10",
      "sass-loader": "^7.1.0",
